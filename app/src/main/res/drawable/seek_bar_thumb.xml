<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners

                android:radius="5dp"
                />
            <size
                android:width="6dp"
                android:height="65dp"/>
        </shape>

    </item>
    <item>
        <shape android:shape="rectangle">

            <size
                android:width="6dp"
                android:height="65dp"/>
            <stroke android:color="@android:color/transparent"
                android:width="4dp"/>
            <corners

             android:radius="1dp"
                />
            <solid android:color="@color/themeMainPrimary1"/>

        </shape>
    </item>

</layer-list>